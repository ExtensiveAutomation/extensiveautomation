def description(self):
	# testcase description
	self.setPurpose(purpose=description('summary'))
	self.setRequirement(requirement=description('requirement'))

	# steps description
	self.step1 = self.addStep(expected="hash %s generated" % input('SHA'),
																			description="generate hash %s" % input('SHA'),
																			summary="generate hash %s" % input('SHA'), 
																			enabled=True)
def prepare(self):
	if input('SHA') == "SHA-1":
		self.LIB_SHA = SutLibraries.Hashing.SHA1(parent=self, name=None, debug=input('DEBUG'), shared=False)
	elif input('SHA') == "SHA-256":
		self.LIB_SHA = SutLibraries.Hashing.SHA256(parent=self, name=None, debug=input('DEBUG'), shared=False)
	elif input('SHA') == "SHA512":
		self.LIB_SHA = SutLibraries.Hashing.SHA512(parent=self, name=None, debug=input('DEBUG'), shared=False)
	else:
		self.abort("sha type not supported: %s" % input('SHA'))	
		
def definition(self):
	# starting initial step
	if self.step1.isEnabled():
		self.step1.start()

		hash = self.LIB_SHA.compute(data=input('DATA_IN'))
		Cache().set(name=input('CACHE_KEY'), data=hash, flag=False)
		
		self.warning("%s: %s" % (input('SHA'),hash) )
		self.step1.setPassed(actual="success")
def cleanup(self, aborted):
	if aborted: self.step1.setFailed(actual=aborted)]]></testdefinition>
